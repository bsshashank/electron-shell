{
  "_id": "_design/shell",
  "version": "0.2.0",
  "views": {
    "extensions": {
      "map": "function mapFun (doc) { if (doc.type === 'extension') { emit(doc.id, doc) } }"
    },
    "settings": {
      "map": "function mapFun (doc) { if (doc.type === 'setting') { emit (doc.extensionid, doc) } }"
    },
    "translations": {
      "map": "function mapFun (doc) { if (doc.type === 'translation') { emit (doc.locale, doc) } }"
    }
  }
}
